{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nikisoft-utils": {
      "projectType": "library",
      "root": "projects/nikisoft-utils",
      "sourceRoot": "projects/nikisoft-utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/nikisoft-utils/tsconfig.lib.json",
            "project": "projects/nikisoft-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nikisoft-utils/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nikisoft-utils/src/test.ts",
            "tsConfig": "projects/nikisoft-utils/tsconfig.spec.json",
            "karmaConfig": "projects/nikisoft-utils/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nikisoft-utils/tsconfig.lib.json",
              "projects/nikisoft-utils/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "nikisoft-utils-playground": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "projects/nikisoft-utils-playground",
      "sourceRoot": "projects/nikisoft-utils-playground/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/nikisoft-utils-playground",
            "index": "projects/nikisoft-utils-playground/src/index.html",
            "main": "projects/nikisoft-utils-playground/src/main.ts",
            "polyfills": "projects/nikisoft-utils-playground/src/polyfills.ts",
            "tsConfig": "projects/nikisoft-utils-playground/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/nikisoft-utils-playground/src/favicon.ico",
              "projects/nikisoft-utils-playground/src/assets"
            ],
            "styles": [
              "projects/nikisoft-utils-playground/src/styles.sass"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/nikisoft-utils-playground/src/environments/environment.ts",
                  "with": "projects/nikisoft-utils-playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nikisoft-utils-playground:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "nikisoft-utils-playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nikisoft-utils-playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/nikisoft-utils-playground/src/test.ts",
            "polyfills": "projects/nikisoft-utils-playground/src/polyfills.ts",
            "tsConfig": "projects/nikisoft-utils-playground/tsconfig.spec.json",
            "karmaConfig": "projects/nikisoft-utils-playground/karma.conf.js",
            "assets": [
              "projects/nikisoft-utils-playground/src/favicon.ico",
              "projects/nikisoft-utils-playground/src/assets"
            ],
            "styles": [
              "projects/nikisoft-utils-playground/src/styles.sass"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/nikisoft-utils-playground/tsconfig.app.json",
              "projects/nikisoft-utils-playground/tsconfig.spec.json",
              "projects/nikisoft-utils-playground/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/nikisoft-utils-playground/e2e/protractor.conf.js",
            "devServerTarget": "nikisoft-utils-playground:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nikisoft-utils-playground:serve:production"
            }
          }
        }
      }
    }},
  "cli": {
    "analytics": "9fce2733-f785-4be8-836d-6d0eb7755d2b"
  },
  "defaultProject": "nikisoft-utils-playground"
}
